/*全真模拟练习*/

.quanzhen {
	width: 80%;
	margin: 0 auto;
	overflow: hidden;
}

.quanzhen p {
	font-size: 1.1em;
	color: #6fbee3;
	line-height: 2.5em;
	text-align: center;
	background: url(../images/quan@2x.png) no-repeat;
	width: 15%;
	float: left;
	background-position: 50%50%;
	background-size: contain;
	margin-top: 0.3rem;
}

.quanzhen span {
	font-size: 1em;
	color: #333;
	line-height: 3.5em;
	margin-left: 20%;
	width: 85%;
	display: block;
}

