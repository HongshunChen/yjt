/*pager*/

.fenye {
	text-align: center;
	height: 52px;
	overflow: hidden;
}

.pager {
	margin: 0;
	padding: 0;
	display: inline-block;

}

.pager-linkPage {
	display: inline-block;
	float: left;
}

.pager .upPage,
.pager .downPage {
	line-height: 20px;
}

.pager .upPage {
	display: inline-block;
	width: 40px;
	height: 22px;
	border: 1px solid #E8E8E6;
}

.pager a,
.pager .pager-linkPage span {
	display: inline-block;
	width: 22px;
	height: 22px;
	line-height: 22px;
	color: #5f5f5f;
	border: 1px solid #E8E8E6;
	margin-left: 6px;
	text-align: center;
	font-size: 10px;
	background: #fff;
}

.pager .pager-linkPage span {
}

.pager a:hover,
.pager .upPage:hover,
.pager .downPage:hover,
.pager .pager-linkPage .current {
	background: #3399FF;
	color: #fff;
	border: 1px solid #3399FF;
}

.pager .downPage {
	display: inline-block;
	width: 40px;
	height: 22px;
	border: 1px solid #E8E8E6;
}

.pager-linkPage,
.pager .downPage,
.pager .upPage,
.pager-linkPage span,
.pager-linkPage a {
	float: left;
}

