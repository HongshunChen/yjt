	width: 6px;
	height: 6px;
	border-radius: 50%;
	margin: 0 5px;
	border: solid white 1px;
	opacity: 0.7;
	/*box-shadow: 0px 0px 4px rgba( 0, 0, 0, 0.2 );*/
	cursor: pointer;
	-webkit-tap-highlight-color: rgba( 0, 0, 0, 0 );
}

.bullets li:hover {
	background: rgba( 255, 255, 255, 0.8 );
}

.bullets li.active {
	cursor: default;
	/*background: #A1D454;*/
	background-color: white;
	border: none;
	width: 8px;
	height: 8px;
	border-radius: 50%;
}

